<div class="tenant-page">
    <!-- starts top navigation bar -->
    <div id="main-top-navbar" attr-user="<%= username %>"></div>
    <!-- header -->
    <div class="row add-margin-bottom add-margin-top">
        <div class="col-md-8">
            <h6 class="frm-bold-text">
                <%= section %>
            </h6>
        </div>
    </div>
    <!-- ends header -->

    <!-- Usage summary -->
    <div class="usage-summary">
        <div class= "container frm-footer-primary">
            <div class="frm-remake col-md-6">
                <h4 class="frm-bold-text">
                    Instance Usage Summary
                </h4>
            </div>
            <!-- INPORTANT: REMOVE - - - Adding just for testing purposes-->
            <div class="col-md-3">
                <div  id="testing-endpoints">
                </div>
            </div>
            <!-- ---------------------------------------------------------->
            <div class="col-md-3">
                <div  id="add-instances">
                </div>
            </div>
        </div>
        <div class="container">
            <!-- Insert summary elements dynamically -->
            <div id="usage-summary" >
            </div>
        </div>
    </div>
    <!-- ends Usage summary -->

    <!-- grouped instances -->
    <div class= "frm-footer-primary add-margin-bottom">
        <div class="frm-remake">
            <h4 class="frm-bold-text add-margin-bottom">
                Grouped Instances
            </h4>
        </div>
    </div>
    <div class="grouped-instances add-margin-bottom">
        <!-- Insert Workloads components dynamically -->
        <div id="workloads-container" class="intances">
        </div>
    </div>
    <!-- end grouped instances -->

    <!-- instances on this host -->
    <a name="instances-overview"></a>
    <div class="instances">
        <div class="row">
            <div class="col-md-12">
                <h2 class="frm-bold-text add-margin-bottom">
                    Instances on this host
                </h2>
            </div>
        </div>
        <div id="instances-host" ></div>
    </div>
    <!-- end instances on this host -->

    <!-- Block devices -->
    <a name="Volumes"></a>
    <div class="instances">
        <div class="row">
            <div class="col-md-12">
                <h2 class="frm-bold-text add-margin-bottom">
                    Volumes
                </h2>
            </div>
        </div>
        <div id="block-catalogue" ></div>
    </div>
</div>
